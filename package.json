{
  "name": "@adstate_as/component-library",
  "description": "Adstate Flora with Storybook. Used to build @adstate_as/flora",
  "author": "Adstate AS",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "pack",
    "README.md"
  ],
  "scripts": {
    "ng": "./node_modules/.bin/ng",
    "build": "npm run build-storybook",
    "build:local": "npm run build",
    "build:development": "npm run build",
    "build:test": "npm run build",
    "build:pre-production": "npm run build",
    "build:production": "npm run build",
    "pack": "./node_modules/.bin/ng build --project=flora --prod && npm run copy-assets",
    "pack:debug": "./node_modules/.bin/ng build --project=flora && npm run copy-assets",
    "publish:auto-version": "npm run semantic-release",
    "publish": "npm publish ./pack",
    "test": "ng test",
    "start": "npm run storybook",
    "lint": "./node_modules/.bin/ng lint",
    "copy-assets": "npm run copy-assets:prepare && npm run copy-assets:styles && npm run copy-assets:icons",
    "copy-assets:prepare": "mkdir ./pack/styles && mkdir -p ./pack/assets/images/flora",
    "copy-assets:styles": "cp -R ./projects/flora/src/styles pack/",
    "copy-assets:icons": "cp -R ./static/assets/images/flora/icons pack/assets/images/flora",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -s ./static -p 6006",
    "build-storybook": "npm run docs:json && build-storybook -s ./static -o ./dist",
    "postinstall": "ngcc",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@angular/animations": "~11.2.14",
    "@angular/cdk": "^11.2.13",
    "@angular/common": "~11.2.14",
    "@angular/compiler": "~11.2.14",
    "@angular/core": "~11.2.14",
    "@angular/flex-layout": "^12.0.0-beta.34",
    "@angular/forms": "~11.2.14",
    "@angular/material": "^11.2.13",
    "@angular/platform-browser": "~11.2.14",
    "@angular/platform-browser-dynamic": "~11.2.14",
    "@angular/router": "~11.2.14",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/gitlab": "^6.2.2",
    "@semantic-release/npm": "^7.1.3",
    "angular-pintura": "file:local_modules/angular-pintura",
    "faker": "^5.5.3",
    "ngx-mask": "^12.0.0",
    "pintura": "file:local_modules/pintura",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "vanilla-colorful": "^0.6.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.7",
    "@angular/cli": "~11.2.14",
    "@angular/compiler-cli": "~11.2.14",
    "@babel/core": "^7.11.6",
    "@compodoc/compodoc": "^1.1.11",
    "@storybook/addon-actions": "^6.3.7",
    "@storybook/addon-essentials": "^6.3.7",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/angular": "^6.3.7",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "babel-loader": "^8.1.0",
    "chromatic": "^5.10.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.1.0",
    "protractor": "~7.0.0",
    "react-is": "^16.13.1",
    "semantic-release": "^17.4.5",
    "storybook-addon-turbo-build": "^1.0.1",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
